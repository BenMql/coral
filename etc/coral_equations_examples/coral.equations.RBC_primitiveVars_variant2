>>Param :: Prandtl := 1.d0 <<
>>Param :: iEkman := 1.d3 <<
>>Param :: RayleighPr := 2.d5 <<     
>>====================================<<
>>Param :: two := 2.d0 <<     
>>====================================<<
>>linear_variable_kxky :: u <<
>>linear_variable_kxky :: v <<
>>linear_variable_kxky :: w <<
>>linear_variable_kxky :: t <<
