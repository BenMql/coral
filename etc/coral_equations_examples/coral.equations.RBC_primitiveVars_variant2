>>add_BC_for_this_var :: 20 <<
>>new_equation :: 2 <<
>>add_d/dt_term  :: + Iz.Iz.tMean <<
>>add_rhs_linear :: + tMean <<
>>add_rhs_NL     :: - Iz.tw <<
